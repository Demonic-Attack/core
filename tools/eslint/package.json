{
    "name": "@demonicattack/eslint",
    "version": "0.0.30",
    "description": "eslint config",
    "keywords": [
        "eslint"
    ],
    "homepage": "https://github.com/Demonic-Attack/core",
    "bugs": {
        "url": "https://github.com/Demonic-Attack/core/issues"
    },
    "repository": {
        "url": "https://github.com/Demonic-Attack/core"
    },
    "license": "MIT",
    "author": {
        "name": "Demonic Attack",
        "url": "https://github.com/demonicattack"
    },
    "type": "module",
    "main": "index.mjs",
    "types": "index.d.ts",
    "files": [
        "node.mjs",
        "react.mjs",
        "next.mjs",
        "index.d.ts",
        "configs"
    ],
    "scripts": {
        "clean": "rm -rf .turbo node_modules",
        "format": "prettier --write \"**/*.{js,mjs,cjs,jsx,mjsx,ts,tsx,mtsx,json,prisma,md}\"",
        "lint": "eslint",
        "lint:fix": "eslint --fix"
    },
    "lint-staged": {
        "*.{js,mjs,cjs,jsx,mjsx,ts,tsx,mtsx,json,prisma,md}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.10.0",
        "@next/eslint-plugin-next": "^14.2.9",
        "eslint": "^9.10.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-eslint-plugin": "^6.2.0",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-jsx-a11y": "^6.10.0",
        "eslint-plugin-no-commented-code": "^1.0.10",
        "eslint-plugin-perfectionist": "^3.5.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^7.1.0",
        "eslint-plugin-react": "^7.35.2",
        "eslint-plugin-react-hooks": "^5.1.0-rc-fb9a90fa48-20240614",
        "eslint-plugin-react-refresh": "^0.4.11",
        "eslint-plugin-regexp": "^2.6.0",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-tailwindcss": "^3.17.4",
        "eslint-plugin-unicorn": "^55.0.0",
        "globals": "^15.9.0",
        "typescript-eslint": "^8.5.0"
    },
    "devDependencies": {
        "@demonicattack/prettier": "workspace:*",
        "@demonicattack/tsconfig": "workspace:*",
        "typescript": "^5.6.2"
    },
    "peerDependencies": {
        "eslint": "^9.10.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
